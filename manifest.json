{
  "manifest_version": 2,
  "name": "Calculator",
  "version": "1.0",
  "description": "Calculator for Google Chrome browser",
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icons/128x128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icons/128x128.png",
    "64": "icons/64x64.png"
  }
}